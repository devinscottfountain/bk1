<div class="w-layout-blockcontainer container w-container"><div class="row"><div class="col col-lg-1 u-md-hide"></div><div class="col col-lg-4 col-md-6 col-sm-12"><div class="product_page-left u-d-flex cc-vertical cc-gap-48"><div class="u-d-flex cc-vertical cc-gap-24"><div class="u-d-flex cc-horizontal cc-between-stretch"><h1 li-object="product.title" class="h6">{{ product.title }}</h1><div class="u-d-flex cc-horizontal cc-gap-8">{% if selected_or_first_available_variant.compare_at_price > selected_or_first_available_variant.price %}    <div li-if="selected_or_first_available_variant.compare_at_price > selected_or_first_available_variant.price">
    <h2 li-object="product.compare_at_price | money" class="h6 u-color-tertiary u-style-strikethrough">{{ product.compare_at_price | money }}</h2>    </div>
{% endif %}<h2 li-object="product.price | money" class="{% if product.compare_at_price > product.price %}u-color-sale{% aelse %}u-color-secondary{% endif %}">{{ product.price | money }}</h2></div></div><p li-object="product.description" class="u-color-secondary">{{ product.description }}</p></div><form>{% form 'product', product %}
