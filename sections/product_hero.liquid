    {% for product-options-loop %}    <div li-for="option in product.options_with_values" li-element="product-options-loop" class="product-header_option">
                                    
                            <div class="product-header_radio-list">{% for product-option-loop %}    <div li-for="value in option.values" li-element="product-option-loop" class="product-header_radio">
                                    <input type="radio" li-element="product-option-input" li-object:value="value" li-object:id="value" class="product-header_radio-button" x-on:click="setVariant" :name="option" :data-name="option" :value="variantName" :id="variantName.toString().normalize('NFD').replace(/[\u0300-\u036f]/g, '').toLowerCase().trim().replace(/\s+/g, '-').replace(/[^\w-]+/g, '').replace(/--+/g, '-')" :checked="selected_or_first_available_variant?.options.includes(variantName)" data-selectorType="radio" :class="firstOptionHasAvailableChildren[variantName] ? '' : 'data-disabled'" value="value" id="value">

                            <label li-element="product-option-name" li-object:for="value" li-object="value" class="product-header_radio-label detail" x-text="variantName" :for="variantName" for="value">XXL</label>
                        </div>
{% endfor %}</div>
                            
                        </div>
{% endfor %}    </div>
{% endif %}<div class="u-d-flex cc-vertical cc-gap-8">{% if add-to-cart %}    <a li-if="product.selected_or_first_available_variant.available = true" li-element="add-to-cart" href="#" class="btn u-w-100 w-button">
    Add to Cart    </a>
{% endif %}{% if product.selected_or_first_available_variant.available != true %}    <div li-if="product.selected_or_first_available_variant.available != true">
    <a href="#" class="btn cc-disabled u-w-100 w-button">Out of Stock</a>    </div>
