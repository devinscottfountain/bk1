<script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="product-variant-container" class="product-item_component u-vflex-stretch-top u-vgap-16" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" x-init="$nextTick(() => { activateVariants() })" @setbundleitem.window="updateBundleItem($event.detail.product, $event.detail.quantity)"><div class="product-item_image-link u-p-relative"><a li-object:href="product.url" href="{{ product.url }}" class="product_item-link w-inline-block"><div class="u-p-relative u-aspect-product"><img src="{{ product.featured_image | img_url: 'master' }}" loading="lazy" li-object:src="product.featured_image | img_url: 'master'" li-object:alt="product.featured_image.alt" alt="{{ product.featured_image.alt }}" class="product_item-image">
<img src="{{ product.metafields.custom.alt_image | img_url: ‘master’ }}" loading="lazy" li-object:src="product.metafields.custom.alt_image | img_url: ‘master’" li-if="product.metafields.custom.alt_image" alt="" class="product_item-image alt">
</div></a>{% if product.selected_or_first_available_variant.available != true %}    <div li-if="product.selected_or_first_available_variant.available != true">
    <div>Sold Out</div>    </div>
{% endif %}{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price">
    On Sale    </div>
{% endif %}</div><a li-object:href="product.url" href="{{ product.url }}" class="u-vflex-left-top u-vgap-4 w-inline-block"><h3 li-object="product.title">{{ product.title }}</h3><div class="u-hflex-left-top u-hgap-8">{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price">
    <div li-object="product.compare_at_price | money" class="u-style-strikethrough">{{ product.compare_at_price | money }}</div>    </div>
{% endif %}<div li-object="product.price | money">{{ product.price | money }}</div></div>{% for value in option.values %}    <div li-for="value in option.values">
                                    
                            <div li-object="li-object">{{ li-object }}</div>
                            
                        </div>
{% endfor %}</a></div>
