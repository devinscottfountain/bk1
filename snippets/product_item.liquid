<a li-object:href="product.url" href="{{ product.url }}" class="product_item-link w-inline-block"><div class="u-p-relative"><div class="u-aspect-3x4"><img class="product_item-image" src="{{ product.featured_image | img_url: 'master' }}" alt="{{ product.featured_image.alt }}" sizes="(max-width: 767px) 92vw, (max-width: 991px) 688px, 900px" li-object:src="product.featured_image | img_url: 'master'" loading="lazy" li-object:alt="product.featured_image.alt" srcset="{{ 'visualelectric-2-p-500.jpeg' | asset_url }} 500w, {{ 'visualelectric-2-p-800.jpeg' | asset_url }} 800w, {{ 'visualelectric-2.jpeg' | asset_url }} 1024w">
{% if product.metafields.custom.alt_image %}    <img class="product_item-image cc-alt" src="worn_with.featured_image | img_url: 'master'" alt="worn_with.featured_image.alt" li-if="product.metafields.custom.alt_image" sizes="(max-width: 767px) 92vw, (max-width: 991px) 688px, 900px" li-object:src="worn_with.featured_image | img_url: 'master'" loading="lazy" li-object:alt="worn_with.featured_image.alt" srcset="{{ 'visualelectric-1-p-500.jpeg' | asset_url }} 500w, {{ 'visualelectric-1-p-800.jpeg' | asset_url }} 800w, {{ 'visualelectric-1.jpeg' | asset_url }} 1024w">
{% endif %}{% if product.metafields.custom.alt_image != true %}    <img class="product_item-image cc-alt" src="product.images[1] | img_url: 'master'" alt="product.title | append: ' alternate view'" li-if="product.metafields.custom.alt_image != true" sizes="(max-width: 767px) 92vw, (max-width: 991px) 688px, 900px" li-object:src="product.images[1] | img_url: 'master'" loading="lazy" li-object:alt="product.title | append: ' alternate view'" srcset="{{ 'visualelectric-1-p-500.jpeg' | asset_url }} 500w, {{ 'visualelectric-1-p-800.jpeg' | asset_url }} 800w, {{ 'visualelectric-1.jpeg' | asset_url }} 1024w">
{% endif %}</div>{% if product.selected_or_first_available_variant.available != true %}    <div li-if="product.selected_or_first_available_variant.available != true" class="product_item-scrim u-p-absolute cc-full u-d-flex cc-vertical cc-center-center u-events-none">
    <div class="u-p-absolute u-color-primary-invert">OUT OF STOCK</div>    </div>
{% endif %}</div>{% if product.selected_or_first_available_variant.available == true %}    <div li-if="product.selected_or_first_available_variant.available == true">
    <h2 li-object="product.title" class="detail">{{ product.title }}</h2><div class="u-d-flex cc-horizontal cc-gap-8">{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price">
    <h3 li-object="product.compare_at_price | money" class="detail u-color-tertiary u-style-strikethrough">{{ product.compare_at_price | money }}</h3>    </div>
{% endif %}<h3 li-object="product.price | money" class="{% if product.compare_at_price > product.price %}u-color-sale{% else %}u-color-secondary{% endif %}">{{ product.price | money }}</h3></div>    </div>
{% endif %}{% if product.selected_or_first_available_variant.available != true %}    <div li-if="product.selected_or_first_available_variant.available != true">
    <h2 li-object="product.title" class="detail u-color-tertiary u-style-strikethrough">{{ product.title }}</h2><div class="u-d-flex cc-horizontal cc-gap-8"><h3 li-object="product.price | money" class="detail u-color-tertiary u-style-strikethrough">{{ product.price | money }}</h3></div>    </div>
{% endif %}</a>
