<script src="https://cdn.jsdelivr.net/npm/gsap@3.12.7/dist/gsap.min.js"></script><script src="https://cdn.jsdelivr.net/npm/gsap@3.12.7/dist/ScrollTrigger.min.js"></script><script type="application/ld+json">{
  "@context": "http://schema.org",
  "@type": "WebPage",
  "url": "{{ shop.url }}",
  "name": "{{ shop.name }}",
  "description": "{{ page_description }}",
  "mainEntity": {
    "@type": "ItemList",
    "name": "New Arrivals",
    "itemListElement": [
      {% for product in collections['new-arrivals'].products limit: 4 %}
      {
        "@type": "ListItem",
        "position": {{ forloop.index }},
        "item": {
          "@type": "Product",
          "name": {{ product.title | json }},
          "url": "{{ shop.url }}{{ product.url }}",
          "image": "{{ product.metafields.custom.alt_image | file_url | default: product.featured_image | img_url: 'master' }}",
          "description": {{ product.description | strip_html | strip_newlines | truncatewords: 150 | json }},
          "offers": {
            "@type": "Offer",
            "price": "{{ product.price | money_without_currency }}",
            "priceCurrency": "{{ shop.currency }}",
            "availability": "{% if product.available %}http://schema.org/InStock{% else %}http://schema.org/OutOfStock{% endif %}"
          }
        }
      }{% unless forloop.last %},{% endunless %}
      {% endfor %}
    ]
  },
  "hasPart": {
    "@type": "CollectionPage",
    "name": "New Arrivals Collection",
    "url": "{{ collections['new-arrivals'].url }}",
    "numberOfItems": {{ collections['new-arrivals'].products.size }}
  }
}
</script><script src="{{ '_helper.js' | asset_url }}" type="text/javascript"></script>{{ content_for_header }}</head>
