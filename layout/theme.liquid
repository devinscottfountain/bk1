  }
  /*Text Selection Color*/
  ::selection, ::target-text  {
    background: var(--_colors---text-selection--background);
    color: var(--_colors---text-selection--text);
    text-shadow: none;
  }
  ::-moz-selection {
    background: var(--_colors---text-selection--background);
    color: var(--_colors---text-selection--text);
    text-shadow: none;
  }
  /*Reset apple form styles*/
  input, textarea, select { 
    -webkit-appearance: none; 
    -moz-appearance: none; 
